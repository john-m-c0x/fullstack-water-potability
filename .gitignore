# Node.js
node_modules/           # Exclude node_modules directory
npm-debug.log           # Exclude npm debug log
yarn-debug.log          # Exclude Yarn debug log
yarn-error.log          # Exclude Yarn error log
package-lock.json       # Exclude npm package-lock file
yarn.lock               # Exclude Yarn lock file

# Build output
build/                  # Exclude build output directory
dist/                   # Exclude distribution output
*.tgz                   # Exclude tarball packages

# Environment variables
.env                    # Exclude environment variable files
.env.local              # Exclude local environment variables
.env.development.local  # Exclude development local environment variables
.env.test.local         # Exclude test local environment variables
.env.production.local    # Exclude production local environment variables

# IDE specific files
.vscode/                # Exclude VSCode settings
.idea/                  # Exclude JetBrains IDE settings
.DS_Store               # Exclude macOS Finder metadata

# Mac system files
Thumbs.db              # Exclude Windows thumbnail cache
ehthumbs.db            # Exclude Windows thumbnail cache
Desktop.ini            # Exclude Windows desktop settings

# Log files
*.log                   # Exclude all log files

# Temporary files
*.tmp                   # Exclude temporary files
*.temp                  # Exclude temp files
*.bak                   # Exclude backup files
*.swp                   # Exclude swap files
*.swo                   # Exclude swap files

# Other
coverage/              # Exclude coverage directory for tests
public/static/         # Exclude static files in public
